@article{Cornips2005,
abstract = {The aim of this article is to provide a first overview on several syntactic elicitation techniques used by dialectologists and sociolinguists and to evaluate them from a generative perspective. Both oral and written techniques will be taken into account and analysed according to the sociolinguistic situation. In addition, basic problems like the choice of subject, the language used for the interaction will be addressed and discussed on the basis of two syntactic Atlases, the Dutch SAND and the Italian ASIS. {\textcopyright} 2004 Elsevier B.V. All rights reserved.},
annote = {The authors discuss reflections on using acceptability judgements to gain insight into dialect variation. They reiterate some of the strengths and weaknesses of using spontaneous speech/ corpora and introspective judgements and then proceed to outline ways to improve on the weaknesses of judgements.

Strengths of speech/ corpora:
- reflect usage (implicit language), provide possible clues to co-varying syntactic variables, appear in cohesive/ communicative contexts

Weaknesses of speech/ corpora:
- construction may be rare (or unattested) and therefore cannot be reliably explored, variables of interest cannot be guaranteed to be present

Strengths of introspection:
- investigators can target specific variables and combinations predicted by a theory to be possible and impossible (negative data), 

Weakeness of introspection:
- Numerous task effects (lexical choice, pragmatic context, processing effects, explicit language attitudes/ bias, etc.

Suggestions for improving introspection (for dialect exploration):

1. Use indirect acceptability judgements (avoid prestige dialect interference).
2. Awareness of the interaction between written forms and dialect usage: prescriptive forms tend to be used in written modality
3. Isolated sentences may lead to a lack of sufficient semantic/ pragmatic cohesiveness. In these cases it is not clear what the participant is judging as they must attempt to contextualize the utterance themselves and this may differ between participants.},
author = {Cornips, Leonie and Poletto, Cecilia},
file = {:Users/francojc/Documents/Mendeley/Cornips, Poletto/2005 - Cornips, Poletto - On standardising syntactic elicitation techniques (part 1).pdf:pdf},
journal = {Lingua},
keywords = {Dialectology,Non-standard varieties,Standardisation,Syntactic elicitation,Syntactic variables},
number = {7},
pages = {939--957},
title = {{On standardising syntactic elicitation techniques (part 1)}},
volume = {115},
year = {2005}
}
