@article{Tabor2004,
abstract = {A central question for psycholinguistics concerns the role of grammatical$\backslash$nconstraints in online sentence processing. Many current theories$\backslash$nmaintain that the language processing mechanism constructs a parse$\backslash$nor parses that are grammatically consistent with the whole of the$\backslash$nperceived input each time it processes a word. Several bottom-up,$\backslash$ndynamical models make a contrasting prediction: partial parses which$\backslash$nare syntactically compatible with only a proper subpart of the input$\backslash$nare sometimes constructed, at least temporarily. Three self-paced$\backslash$nreading experiments probed for interference from such locally coherent$\backslash$nstructures. The first tested for a distracting effect of irrelevant$\backslash$nSubject-Predicate interpretations of Noun Phrase-Verb Phrase sequences$\backslash$n(e.g., The coach smiled at ) on reading times. The second addressed$\backslash$nthe question of whether the interference effects can be treated as$\backslash$nlexical interference, instead of involving the formation of locally$\backslash$ncoherent syntactic structures. The third replicated the reading time$\backslash$neffects of the first two experiments with grammaticality judgments.$\backslash$nWe evaluate the dynamical account, comparing it to other approaches$\backslash$nthat also predict effects of local coherence, and arguing against$\backslash$naccounts which rule out the formation of merely locally coherent$\backslash$nstructures.},
author = {Tabor, W and Galantucci, B and Richardson, D},
file = {:Users/francojc/Documents/Mendeley/Tabor, Galantucci, Richardson/2004 - Tabor, Galantucci, Richardson - Effects of Merely Local Syntactic Coherence on Sentence Processing.pdf:pdf},
title = {{Effects of Merely Local Syntactic Coherence on Sentence Processing.}},
url = {http://discovery.ucl.ac.uk/98052/},
year = {2004}
}
