abstract = {Background: This study investigates the role of lexical information in$\backslash$nnormal and aphasic sentence comprehension. Effects of verb biases in$\backslash$nnormal comprehension have been well documented in previous studies (e.$\backslash$ng., Spivey-Knowlton Sedivy, 1995; Trueswell, Tanenhaus, {\&} Kello,$\backslash$n1993), but their role in aphasic language processing has largely been$\backslash$nignored (with the exceptions of Menn et al., 1998, and Russo, Peach, {\&}$\backslash$nShapiro, 1998).$\backslash$nAims: The aim of the study is to test the lexical bias hypothesis,$\backslash$ni.e., the hypothesis that sentence comprehension is influenced by$\backslash$nlexical biases in aphasic listeners, as well as in normals.$\backslash$nMethod {\&} Procedures: Using a sentence plausibility judgement task, we$\backslash$nprobe for sensitivity to verb transitivity bias, i.e., the likelihood,$\backslash$nas estimated from corpus counts, that a verb will be transitive, rather$\backslash$nthan intransitive. Five normal controls and eighteen participants with$\backslash$naphasia (six with Broca's aphasia, four with Wernicke's aphasia, two$\backslash$nwith conduction aphasia, and six with anomic aphasia) are included in$\backslash$nthe study. Based on the lexical bias hypothesis, we predicted that$\backslash$nparticipants would make more errors in sentences with a mismatch of$\backslash$nverb bias and syntactic structure, such as a transitive sentence$\backslash$ncontaining a verb with intransitive bias.$\backslash$nOutcomes {\&} Results: Both the group of normal controls and the mixed$\backslash$ngroup of aphasic patients make significantly more errors on sentences$\backslash$nin which there is a mismatch between verb bias and syntactic structure,$\backslash$nas predicted by the lexical bias hypothesis. Specifically, patients$\backslash$nwith fluent aphasia types, particularly anomic aphasia, show a$\backslash$nsensitivity to verb bias, contrary to earlier findings.$\backslash$nConclusions: These results are consistent with the view that lexical$\backslash$nfactors, not purely syntactic ones, are to blame for many previously$\backslash$nobserved patterns in aphasic comprehension. The results are further$\backslash$nconsistent with the view that many aphasic errors differ not$\backslash$nqualitatively but quantitatively from normal comprehension errors.},
author = {Gahl, Susanne},
file = {:Users/francojc/Documents/Mendeley/Gahl/2002 - Gahl - Lexical biases in aphasic sentence comprehension An experimental and corpus linguistic study.pdf:pdf},
journal = {Aphasiology},
number = {12},
pages = {1173--1198},
title = {{Lexical biases in aphasic sentence comprehension: An experimental and corpus linguistic study}},
volume = {16},
year = {2002}
}
