@article{arnon2010more,
abstract = {There is mounting evidence that language users are sensitive to distributional information at many grain-sizes. Much of this research has focused on the distributional properties of words, the units they consist of (morphemes, phonemes), and the syntactic structures they appear in (verb-categorization frames, syntactic constructions). In a series of studies we show that comprehenders are also sensitive to the frequencies of compositional four-word phrases (e.g. don't have to worry): more frequent phrases are processed faster. The effect is not reducible to the frequency of the individual words or substrings and is observed across the entire frequency range (for low, mid and high frequency phrases). Comprehenders seem to learn and store frequency information about multi-word phrases. These findings call for processing models that can capture and predict phrase-frequency effects and support accounts where linguistic knowledge consists of patterns of varying sizes and levels of abstraction. {\textcopyright} 2009 Elsevier Inc. All rights reserved.},
annote = {This article offers interesting evidence suggesting that binning frequency is wrong, and that frequency is best treated as a continuous variable. Furthermore frequency effects are shown not only at higher frequency, but throughout the spectrum, from low to high frequency.

This article deals with multi-word expressions and made me think of a couple research questions:

1) Lexical diversity, frequency scores and frequency effects
Underlines how 'frequency' as a score is a function of the lexical diversity of a corpus. Arnon {\&} Snider do not discuss how their results might differ if the lexical diversity of the corpus was lower -- note, that they do use the Switchboard {\&} Fisher corpus (which is lower in lexical diversity) but they select 4 grams, which be their nature will be more diverse that the unigram frequencies of the corpus. Even then, a more lexically diverse sample (written) would produce even more diverse 4 grams (I suppose)

2) Sub-syntactic (chunks). 
I started thinking about the possibility of linking my satiation studies thinking about multi-word frequency measures as a way to see how predictable some sub-structures are and to what degree the whole syntactic meaning could be predicted based on this sub-structural information.},
author = {Arnon, Inbal and Snider, Neal},
doi = {10.1016/j.jml.2009.09.005},
file = {:Users/francojc/Documents/Mendeley/Arnon, Snider/2010 - Arnon, Snider - More than words Frequency effects for multi-word phrases.pdf:pdf},
isbn = {0749-596X},
issn = {0749596X},
journal = {Journal of Memory and Language},
keywords = {Comprehension,Frequency,Lexicon,Ngram,Usage-based models},
number = {1},
pages = {67--82},
pmid = {19261173},
publisher = {Elsevier},
title = {{More than words: Frequency effects for multi-word phrases}},
url = {http://www.sciencedirect.com/science/article/pii/S0749596X09000965},
volume = {62},
year = {2010}
}
