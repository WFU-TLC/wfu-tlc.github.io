@article{Fox2007,
abstract = {{\textless}p{\textgreater}This paper is a usage-based study of the grammar of that set of English Relative Clauses with which a relativizer has been described as optional. We argue that the regularities in the use of relativizers in English can be seen as systematically arising from pragmatic-prosodic factors, creating frequency effects, resulting in some cases highly grammaticized formats: the more the Main Clause and the Relative Clause are integrated with each other, that is, approach monoclausal status, the more likely we are to find no relativizer used; conversely, the more separate the two clauses are, the more likely we are to find an overt relativizer. These findings have led us to suggest that the more monoclausal combinations have become unitary storage and processing chunks. We thus see these findings as a contribution not only to our understanding of Relative Clauses, but to our understanding of syntactic organization in general and of the nature of the grammatical practices in which speakers engage in everyday interactions.{\textless}/p{\textgreater}},
annote = {The authors argue that the semantic closeness of the main and relative clauses predicts the use of an overt relativizer: 

No relativizer

- more monoclausal end having an almost empty Main
Clause, simple head NP and short ORC, with highly integrated rhythm, and displaying a range from extreme lexical specificity (memorized chunk) to a somewhat more general pattern with still a fair amount of lexical specificity;

Relativizer

- biclausal pattern tends to be made up of less frequent lexical items, and is less likely to be composed of
pre-stored chunks or low-level formats with lexically specific components. This end of the continuum shows a greater preference for an overt relativizer.},
author = {Fox, Barbara A. and Thompson, Sandra A.},
doi = {10.1075/sl.31.2.03fox},
file = {:Users/francojc/Documents/Mendeley/Fox, Thompson/2007 - Fox, Thompson - Relative Clauses in English conversation Relativizers, frequency, and the notion of construction.pdf:pdf},
isbn = {9780511843433},
issn = {0378-4177},
journal = {Studies in Language},
number = {2},
pages = {293--326},
title = {{Relative Clauses in English conversation: Relativizers, frequency, and the notion of construction}},
url = {http://www.jbe-platform.com/content/journals/10.1075/sl.31.2.03fox},
volume = {31},
year = {2007}
}
