journal = {Behavior Research Methods},
number = {1},
pages = {213--231},
pmid = {18411545},
title = {{Word associations: Network and semantic properties}},
url = {http://link.springer.com/10.3758/BRM.40.1.213},
volume = {40},
year = {2008}
}
