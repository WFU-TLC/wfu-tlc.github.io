@article{Greenbaum1977,
abstract = {Three experiments were performed investigating: (1) the feasibility of using frequency judgments by native speakers for syntactic descriptions of Eng, (2) the possibility of a relationship between frequency {\&} acceptability judgments, {\&} (3) some linguistic variables influencing changes in perceived frequencies {\&} acceptability. Ss were Wisc Coll freshmen {\&} sophomores. One hundred sentences were divided into eight groups, each dealing with a particular type of syntactic variation. Sentences were presented in pairs {\&} Ss were asked to rate each sentence on a five-point acceptability or frequency scale. An intra-S consistency index was devised: Ss were generally consistent on both kinds of judgments between lexically different but syntactically identical sentences if a one-position difference on the five-place scale is allowed. The same measure of consistency was arrived at in comparing frequency vs acceptability judgments. An analysis of variance showed the reliability of the group to be quite high: results support the hypothesis that active sentences are judged to be both more frequent {\&} more acceptable than passive sentences. Sentences with might both positive {\&} negative were judged to be somewhat more frequent than sentences with may, although acceptability was the same for both types. The simple past was shown to be more readily interchangeable with the present perfect when the reference was to a series of actions rather than to a state that existed continuously during the previous period. The subjunctive was felt to be more frequent {\&} more acceptable than either the indicative or should after Vs expressing both demands {\&} persuasion. The perception of the normal position of time {\&} place adverbials conformed to expectations (place adjuncts precede time adjuncts which come in the following sequence: duration-frequency-when). The contraction haven't was favored over they've {\&} the contraction of is was preferred over the contraction of not. Subjects {\&} auxiliaries were ellipted if they were both identical-no ellipsis occurred if the auxiliaries were identical {\&} the subjects different. The indirect O construction was favored over the to construction when the indirect O was introduced by the {\&} conversely when it was introduced by a. Instructions for the frequency experiment are given in an appendix. A. Orianne},
author = {Greenbaum, Sidney},
doi = {10.1111/j.1467-9582.1977.tb00649.x},
file = {:Users/francojc/Documents/Mendeley/Greenbaum/1977 - Greenbaum - Judgments of Syntactic Acceptability and Frequency.pdf:pdf},
issn = {14679582},
journal = {Studia Linguistica},
number = {2},
pages = {83--105},
title = {{Judgments of Syntactic Acceptability and Frequency}},
volume = {31},
year = {1977}
