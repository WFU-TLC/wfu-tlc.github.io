@article{Stefanowitsch2006,
abstract = {It is argued that introspective judgments of acceptability can {\&} should be avoided by linguists, as the method of simple collexeme analysis, the most basic of a set of methods developed in the framework of construction grammar by Stefanowitsch {\&} Stefan Th. Gries {\&} known collectively as collostructional analysis, permits the identification of negative evidence in large corpora {\&} thereby enables linguists to construct truly scientific, testable hypotheses regarding structures that cannot occur in a language. The method is illustrated in application to the relation between English verbs {\&} the ditransitive construction in the British component of the International Corpus of English {\&} in the larger British National Corpus; the expected frequency of a particular verb in the construction is calculated from the four observed frequencies of (1) the word in the construction, (2) other verbs in the construction, (3) the given verb in all other constructions, {\&} (4) all other verbs in all other constructions. Verbs that fail to occur in the ditransitive construction, eg say, count as negative evidence if their calculated expectation of occurrence is sufficiently high.},
author = {Stefanowitsch, Anatol},
