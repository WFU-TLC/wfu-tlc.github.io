@article{Shimpi2007,
abstract = {The current studies used a syntactic priming paradigm with 3- and 4-year-old children. In Experiment 1, children were asked to describe a series of drawings depicting transitive and dative relations to establish baseline production levels. In Experiment 2, an experimenter described a similar series of drawings using one of two syntactic forms (i.e., active/passive for transitive; double-object/prepositional for dative). Children were then asked to describe pictures identical to those shown in the corresponding baseline procedure. In both transitive and dative conditions, 4-year-old children were more likely to use a particular syntactic form if it had been used by the experimenter. Three-year-old children did not show priming effects, but their production of transitive sentences was higher following transitive primes than in Experiment 1. In Experiment 3, an additional group of 3-year-olds participated in a procedure in which they repeated the experimenter's sentences before describing the pictures. This procedure yielded significant priming effects for transitive and dative forms. These results indicate that very young children possess abstract syntactic representations, but that their access to these representations is sensitive to task demands.},
author = {Shimpi, Priya M and G{\'{a}}mez, Perla B and Huttenlocher, Janellen and Vasilyeva, Marina},
institution = {Department of Psychology, University of Chicago, Santa Cruz, CA 95064, USA. pshimpi@ucsc.edu},
journal = {Developmental Psychology},
keywords = {*language,*language development,*methods,*semantics,child,female,humans,language tests,male,models,preschool,psychological,teaching},
number = {6},
pages = {1334--1346},
pmid = {18020815},
title = {{Syntactic priming in 3- and 4-year-old children: evidence for abstract representations of transitive and dative forms.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18020815},
volume = {43},
year = {2007}
}
