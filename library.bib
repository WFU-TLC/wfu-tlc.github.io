@inproceedings{Snow2008,
abstract = {Human linguistic annotation is crucial for many natural language processing tasks but can be expensive and time-consuming.We explore the use of Amazon's Mechanical Turk system, a significantly cheaper and faster method for collecting annotations from a broad base of paid non-expert contributors over the Web. We investigate five tasks: af- fect recognition, word similarity, recognizing textual entailment, event temporal ordering, and word sense disambiguation. For all five, we show high agreement between Mechani- cal Turk non-expert annotations and existing gold standard labels provided by expert label- ers. For the task of affect recognition, we also show that using non-expert labels for training machine learning algorithms can be as effec- tive as using gold standard annotations from experts. We propose a technique for bias correction that significantly improves annota- tion quality on two tasks. We conclude that many large labeling tasks can be effectively designed and carried out in this method at a fraction of the usual expense.},
author = {Snow, Rion and O'Connor, Brendan and Jurafsky, Daniel and Ng, Andrew Y.},
booktitle = {Proceedings of the Conference on Empirical Methods in Natural Language Processing},
file = {:Users/francojc/Documents/Mendeley/Snow et al/2008 - Snow et al. - Cheap and fast---but is it good evaluating non-expert annotations for natural language tasks.pdf:pdf},
keywords = {annotation,human linguistic annotation,mechanical turk,methodology,nlp},
mendeley-tags = {annotation,human linguistic annotation,mechanical turk,methodology,nlp},
pages = {254--263},
title = {{Cheap and fast---but is it good?: evaluating non-expert annotations for natural language tasks}},
url = {http://dl.acm.org/citation.cfm?id=1613751},
year = {2008}
}
@article{Gibson2013,
abstract = {The prevalent method in syntax and semantics research involves obtaining a judgement of the acceptability of a sentence/meaning pair, typically by just the author of the paper, sometimes with feedback from colleagues. This methodol-ogy does not allow proper testing of scientific hypotheses because of (a) the small number of experimental participants (typically one); (b) the small number of experimental stimuli (typically one); (c) cognitive biases on the part of the researcher and participants; and (d) the effect of the preceding context (e.g., other constructions the researcher may have been recently considering). In the current paper we respond to some arguments that have been given in support of continuing to use the traditional nonquantitative method in syntax/semantics research. One recent defence of the traditional method comes from Phillips (2009), who argues that no harm has come from the nonquantitative approach},
annote = {The authors argue that traditional methodological approaches to syntactic evidence is flawed. They advocate for experimental practices to mitigate errors introduced in these methods in the areas of linguistic stimuli, participants and investigators.

Section 2: empirical investigations

Two ways to gather data in any scientific domain:
- naturalistic observation (corpus studies included here)
* Advocates: Bresnan 2007, Bresnan {\&} Nikitina 2009, Labov 1975, 1996)
- controlled experiments (experimental syntax, behavioral methods here)

Section 3: arguments in favor of non-quantitative methods

1. Difficultly of gathering quantitative data
2. Viability of small number of participants in other domains
3. Quantitative evidence is also inconclusive, so why bother?
4. Quantitative evidence is insufficient and slows progress
5. Linguists make better participants (avoid non-relevant factors)
- Cognitive bias: confirmation bias (researcher and/or participant)
6. Public forums perform the vetting process
7. No harm has come from non-quantitative approaches

Section 4: faulty judgments in the literature

- Dative alternation/ extraction (Bresnan {\&} Nikitina, 2009)
- Syntactic flexibility of idioms
- Heavy NP shift

Section 5: subject/ objective relative clauses
Section 6: multiple wh-extraction effects},
author = {Gibson, Edward and Fedorenko, Evelina},
file = {:Users/francojc/Documents/Mendeley/Gibson, Fedorenko/2013 - Gibson, Fedorenko - The need for quantitative methods in syntax and semantics research.pdf:pdf},
journal = {Language and Cognitive Processes},
number = {1-2},
pages = {88--124},
title = {{The need for quantitative methods in syntax and semantics research}},
volume = {28},
year = {2013}
}
