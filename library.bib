file = {:Users/francojc/Documents/Mendeley/Kulkarni et al/2015 - Kulkarni et al. - Statistically significant detection of linguistic change.pdf:pdf},
isbn = {9781450334693},
journal = {Proceedings of the 24th international conference on World Wide Web (WWW '15)},
keywords = {computational linguistics,web mining},
pages = {625--635},
title = {{Statistically significant detection of linguistic change}},
url = {http://dl.acm.org/citation.cfm?id=2741627},
year = {2015}
